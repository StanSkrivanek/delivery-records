# Local development environment variables
# Update values as needed. Do not commit secrets to Git.

# Postgres (Supabase local)
DATABASE_URL=postgresql://postgres:postgres@127.0.0.1:55422/postgres

# Supabase endpoints (local)
SUPABASE_URL=http://127.0.0.1:55421
# Replace with your local anon key (do not commit the real value)
SUPABASE_ANON_KEY={{SUPABASE_ANON_KEY}}
# Replace with your local service role key (do not commit the real value)
SUPABASE_SERVICE_ROLE_KEY={{SUPABASE_SERVICE_ROLE_KEY}}

# Better Auth secret (generate and keep private)
BETTER_AUTH_SECRET={{BETTER_AUTH_SECRET}}
